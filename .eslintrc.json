{
  "extends": [
    "next/core-web-vitals",
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:storybook/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint", "testing-library"],
  "root": true,
  "rules": {
    "no-console": "warn",
    "testing-library/no-debugging-utils": "error"
  }
}
