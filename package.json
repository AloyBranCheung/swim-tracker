{
  "name": "swim-tracker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:dev": "vitest --watch",
    "generate-client": "npx prisma generate",
    "migrate:dev": "dotenv -e .env.local -- npx prisma migrate dev --name",
    "docker:dev": "docker compose -f docker-compose.dev.yml up -d --build",
    "docker:logs": "docker compose logs",
    "docker:dev-stop": "docker compose -f docker-compose.dev.yml down",
    "docker:restart": "yarn docker:dev-stop && yarn docker:dev"
  },
  "dependencies": {
    "@prisma/client": "^5.10.2",
    "framer-motion": "^11.0.8",
    "next": "14.1.3",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.51.0",
    "tailwind-merge": "^2.2.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^8",
    "eslint-config-next": "14.1.3",
    "jsdom": "^24.0.0",
    "postcss": "^8.4.35",
    "prettier": "3.2.5",
    "prisma": "^5.10.2",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "vitest": "^1.3.1"
  }
}
