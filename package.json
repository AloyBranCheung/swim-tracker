{
  "name": "swim-tracker",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "yarn prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:dev": "vitest --watch",
    "generate-client": "yarn prisma generate",
    "db:push": "dotenv -e .env.local -- yarn prisma db push",
    "migrate:dev": "yarn generate-client && dotenv -e .env.local -- yarn prisma migrate dev",
    "migrate:deploy": "dotenv -e .env.local -- yarn prisma migrate deploy",
    "migrate:data": "./prisma/data-migrations/migrate.sh",
    "docker:dev": "docker compose -f docker-compose.dev.yml up -d --build",
    "docker:logs": "docker compose -f docker-compose.dev.yml logs -f",
    "docker:dev-stop": "docker compose -f docker-compose.dev.yml down",
    "docker:restart": "yarn docker:dev-stop && yarn docker:dev",
    "dotenv": "dotenv -e .env.local --",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^3.5.0",
    "@prisma/client": "^5.10.2",
    "axios": "^1.6.7",
    "dayjs": "^1.11.10",
    "dotenv": "^16.4.5",
    "framer-motion": "^11.0.8",
    "lodash": "^4.17.21",
    "next": "14.1.3",
    "pino": "^8.19.0",
    "react": "^18",
    "react-dom": "^18",
    "styled-components": "^6.1.8",
    "tailwind-merge": "^2.2.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "1.2.25",
    "@mermaid-js/mermaid-cli": "^10.8.0",
    "@storybook/addon-essentials": "^8.0.4",
    "@storybook/addon-interactions": "^8.0.4",
    "@storybook/addon-links": "^8.0.4",
    "@storybook/addon-onboarding": "^8.0.4",
    "@storybook/addon-storysource": "^8.0.4",
    "@storybook/blocks": "^8.0.4",
    "@storybook/nextjs": "^8.0.4",
    "@storybook/react": "^8.0.4",
    "@storybook/test": "^8.0.4",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^8",
    "eslint-config-next": "14.1.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-storybook": "^0.8.0",
    "jsdom": "^24.0.0",
    "pino-pretty": "^10.3.1",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.12",
    "prisma": "^5.10.2",
    "prisma-erd-generator": "^1.11.2",
    "storybook": "^8.0.4",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.7.1",
    "typescript": "^5",
    "typescript-eslint": "^7.3.1",
    "vitest": "^1.3.1"
  }
}
